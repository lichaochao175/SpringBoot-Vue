<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcc.springbootvue.mapper.ReturnOrderMapper">

    <select id="getReturnOrderNum" resultType="java.lang.Integer">

        select c from  return_order
        <where>
            1=1
            <if test="query.month !=null or query.month != ''">
                AND DATE_FORMAT( #{query.month}, '%Y%m' ) = DATE_FORMAT( CURDATE( ) , '%Y%m' )
            </if>

        </where>

    </select>


</mapper>